{
    "configurations": [
        {
            "name": "Linux-ariacoda-rosmelodic",
            "includePath": [
                "${workspaceFolder}/**",
                "../AriaCoda/include",
                "/opt/ros/melodic/include/**"
            ],
            "defines": [
              "MOBILESIM_PIONEER",
